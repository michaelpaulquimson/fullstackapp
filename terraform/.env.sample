# AWS credentials for Terraform and local development
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_DEFAULT_REGION=us-east-1
AWS_PROFILE=default

# (Optional) ECR repository URL for CI/CD and Terraform
ECR_REPOSITORY=your-account-id.dkr.ecr.us-east-1.amazonaws.com/your-repo

# (Optional) Lambda role ARN
LAMBDA_ROLE_ARN=arn:aws:iam::your-account-id:role/your-lambda-role
